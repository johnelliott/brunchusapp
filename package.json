{
  "name": "brunch.us",
  "version": "1.0.0",
  "description": "web interface for brunch.us",
  "main": "index.js",
  "scripts": {
    "api-debug": "DEBUG=* ENV=development node_modules/nodemon/bin/nodemon.js bin/www",
    "api-dev": "ENV=development node_modules/nodemon/bin/nodemon.js bin/www",
    "api-inspect": "DEBUG=* ENV=development node_modules/nodemon/bin/nodemon.js --inspect bin/www",
    "start": "node ./bin/www",
    "redis": "redis-server --loglevel verbose",
    "build-client": "node_modules/webpack/bin/webpack.js -p --config webpack.config.js && npm run build-sw",
    "build-sw": "node_modules/sw-precache/cli.js --root=tmp --static-file-globs='public/**/*.{js,css,html,png,ico}' && cp tmp/service-worker.js public/",
    "build-sw-production": "node_modules/sw-precache/cli.js --root=tmp --static-file-globs='public/**/*.{js,css,html,png,ico}' && cat tmp/service-worker.js | ./node_modules/uglify-js/bin/uglifyjs > public/service-worker.js",
    "build-client-dev": "node_modules/webpack/bin/webpack.js --config webpack.dev.config.js",
    "watch-client": "node_modules/webpack/bin/webpack.js --watch --config webpack.dev.config.js",
    "live-client": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.live.config.js",
    "test": "echo NO TESTS!",
    "clean": "rm public/*.{js,css}.map public/*.{js,css}"
  },
  "keywords": [
    "node",
    "brunch"
  ],
  "author": "John Elliott <johnelliott703@gmail.com>",
  "license": "All rights reserved",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "ip": "^1.1.3",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "nano": "^6.2.0",
    "node-sass": "^3.8.0",
    "node-uuid": "^1.4.7",
    "pug": "^2.0.0-beta6",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "request": "^2.75.0",
    "serve-favicon": "^2.3.0",
    "swipe-cards": "^1.0.0",
    "twilio": "^2.9.3",
    "uglify-js": "^2.6.4",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "css-loader": "^0.25.0",
    "nodemon": "^1.10.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "sw-precache": "^4.1.0",
    "tape": "^4.4.0",
    "webpack-dev-server": "^1.16.1"
  }
}
