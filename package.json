{
  "name": "brunch.us",
  "version": "1.0.0",
  "description": "web interface for brunch.us",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-production",
    "build-client": "browserify client/index.js --outfile public/bundle.js -t babelify",
    "build-client-uglified": "browserify client/index.js -t babelify | ./node_modules/uglify-js/bin/uglifyjs > public/bundle.js",
    "build-css": "node_modules/node-sass/bin/node-sass client/style.scss public/style.css",
    "build-min-css": "npm run build-css && node_modules/clean-css/bin/cleancss public/style.css -o public/style.css",
    "build-production": "npm run build-min-css && npm run build-client-uglified && npm run compress-assets",
    "compress-assets": "gzip -kf public/*.js && gzip -kf public/*.css",
    "dbg-api": "DEBUG=* ENV=development node_modules/nodemon/bin/nodemon.js bin/www",
    "dbg-ui": "node_modules/budo/bin/cmd.js client/index.js --serve bundle.js --live --dir public/ -- -t babelify --outfile public/bundle.js",
    "dev-webpack": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.live.config.js --open",
    "start-api": "node ./bin/www",
    "inspect-api": "DEBUG=* ENV=development node_modules/nodemon/bin/nodemon.js --inspect bin/www",
    "test": "echo NO TESTS!",
    "watch-client": "node_modules/watchify/bin/cmd.js --debug client/index.js --outfile public/bundle.js -t babelify",
    "watch-client-webpack": "node_modules/webpack/bin/webpack.js --watch --config webpack.dev.config.js",
    "build-client-webpack": "node_modules/webpack/bin/webpack.js -p --config webpack.dev.config.js",
    "watch-css": "node_modules/node-sass/bin/node-sass --watch client/style.scss public/style.css",
    "webpack": "node_modules/webpack/bin/webpack.js"
  },
  "keywords": [
    "node",
    "brunch"
  ],
  "author": "John Elliott <johnelliott703@gmail.com>",
  "license": "All rights reserved",
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.0.0",
    "clean-css": "^3.4.18",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debug": "^2.2.0",
    "ecstatic": "^1.4.1",
    "express": "^4.14.0",
    "ip": "^1.1.3",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-sass": "^3.8.0",
    "node-uuid": "^1.4.7",
    "pug": "^2.0.0-beta6",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "request": "^2.75.0",
    "serve-favicon": "^2.3.0",
    "swipe-cards": "^1.0.0",
    "twilio": "^2.9.3",
    "uglify-js": "^2.6.4",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "budo": "^8.3.0",
    "nodemon": "^1.10.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "standard": "^8.0.0",
    "tape": "^4.4.0",
    "watchify": "^3.7.0"
  }
}
