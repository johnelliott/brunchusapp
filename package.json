{
  "name": "brunch.us",
  "version": "1.0.0",
  "description": "web interface for brunch.us",
  "main": "index.js",
  "scripts": {
    "api-debug": "DEBUG=* ENV=development node_modules/nodemon/bin/nodemon.js bin/www",
    "api-dev": "ENV=development node_modules/nodemon/bin/nodemon.js bin/www",
    "api-inspect": "DEBUG=* ENV=development node_modules/nodemon/bin/nodemon.js --inspect bin/www",
    "start": "node ./bin/www",
    "build-client": "node_modules/webpack/bin/webpack.js -p --config webpack.config.js",
    "build-client-dev": "node_modules/webpack/bin/webpack.js --config webpack.dev.config.js",
    "watch-client": "node_modules/webpack/bin/webpack.js --watch --config webpack.dev.config.js",
    "live-client": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.live.config.js --open",
    "test": "echo NO TESTS!",
    "clean": "rm public/*.{js,css}.map public/*.{js,css}"
  },
  "keywords": [
    "node",
    "brunch"
  ],
  "author": "John Elliott <johnelliott703@gmail.com>",
  "license": "All rights reserved",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "ip": "^1.1.3",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "nano": "^6.2.0",
    "node-sass": "^3.8.0",
    "node-uuid": "^1.4.7",
    "pug": "^2.0.0-beta6",
    "react-router": "^2.8.1",
    "request": "^2.75.0",
    "serve-favicon": "^2.3.0",
    "swipe-cards": "^1.0.0",
    "twilio": "^2.9.3",
    "uglify-js": "^2.6.4",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "css-loader": "^0.25.0",
    "nodemon": "^1.10.2",
    "sass-loader": "^4.0.2",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "tape": "^4.4.0"
  }
