{
  "name": "wb-web",
  "version": "1.0.0",
  "description": "web interface for waybot",
  "main": "index.js",
  "scripts": {
    "two-fake-counters": "echo 88 89 |xargs -P 2 -n 1 ./faker.sh",
    "dbg": "DEBUG=wb* node_modules/nodemon/bin/nodemon.js index.js --ignore src/ --ignore public/*.js",
    "watch": "node_modules/watchify/bin/cmd.js --debug src/index.js --outfile public/bundle.js -t babelify",
    "sass-watch": "node_modules/node-sass/bin/node-sass --watch src/style.scss public/style.css",
    "build-css": "node_modules/node-sass/bin/node-sass src/style.scss public/style.css",
    "build-min-css": "npm run build-css && node_modules/clean-css/bin/cleancss public/style.css -o public/style.css",
    "budo": "node_modules/budo/bin/cmd.js src/index.js --serve bundle.js --live --dir public/ -- -t babelify --outfile public/bundle.js",
    "build": "npm run build-production",
    "build-client": "browserify src/index.js --outfile public/bundle.js -t babelify",
    "build-client-uglified": "browserify src/index.js -t babelify | ./node_modules/uglify-js/bin/uglifyjs > public/bundle.js",
    "build-production": "npm run build-min-css && npm run build-client-uglified && npm run compress-assets",
    "compress-assets": "gzip -kf public/*.js && gzip -kf public/*.css && gzip -kf public/*/*.jpg && gzip -kf public/*/*/*.svg && gzip -kf public/*/*/*.ttf",
    "test": "echo NO TESTS!",
    "start": "node index.js"
  },
  "keywords": [
    "node",
    "brunch"
  ],
  "author": "John Elliott <johnelliott703@gmail.com>",
  "license": " all rights reserved",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "clean-css": "^3.4.18",
    "debug": "^2.2.0",
    "ecstatic": "^1.4.1",
    "node-sass": "^3.8.0",
    "request": "^2.74.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "budo": "^8.3.0",
    "nodemon": "^1.10.2",
    "tape": "^4.4.0",
    "uglify-js": "^2.6.4",
    "watchify": "^3.7.0"
  }
}
